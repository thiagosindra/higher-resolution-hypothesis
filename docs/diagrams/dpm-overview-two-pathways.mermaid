%%{init: {
  'theme': 'base',
  'themeVariables': {
    'fontFamily': 'Inter, system-ui, sans-serif',
    'fontSize': '16px',
    'lineColor': '#cbd5e1',
    'edgeLabelBackground': 'transparent',
